{
  "name": "react-ci-cd-sample",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "tsc && ANALYZE=true vite build",
    "size-limit": "size-limit",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "deploy:production": "echo Deploying to production...",
    "deploy:staging": "echo Deploying to staging..."
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@playwright/test": "1.45.1",
    "@typescript-eslint/eslint-plugin": "7.16.1",
    "@typescript-eslint/parser": "7.16.1",
    "@vitejs/plugin-react": "4.3.1",
    "@testing-library/jest-dom": "6.4.3",
    "@testing-library/react": "14.2.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.10",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "eslint": "8.57.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "identity-obj-proxy": "3.0.0",
    "jest-environment-jsdom": "29.7.0",
    "jest": "29.7.0",
    "ts-node": "10.9.2",
    "ts-jest": "29.1.2",
    "typescript": "5.5.3",
    "vite": "5.3.4",
    "rollup-plugin-visualizer": "5.12.0",
    "size-limit": "11.1.5",
    "@size-limit/file": "11.1.5"
  },
  "size-limit": [
    {
      "name": "App bundle",
      "path": "dist/assets/*.js",
      "limit": "200 KB",
      "import": "@size-limit/file"
    }
  ]
}
